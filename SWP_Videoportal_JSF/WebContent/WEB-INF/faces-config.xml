<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>userDataMB</managed-bean-name>
  <managed-bean-class>de.mb.UserDataMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>topicMB</managed-bean-name>
  <managed-bean-class>de.mb.TopicMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>subcategoryMB</managed-bean-name>
  <managed-bean-class>de.mb.SubcategoryMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>videoMB</managed-bean-name>
  <managed-bean-class>de.mb.VideoMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/protected/public/startseite.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>userMenue</from-outcome>
   <to-view-id>/protected/admin/userMenue.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>videoVerwaltung</from-outcome>
   <to-view-id>/protected/admin/videoVerwaltung.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>topicMenue</from-outcome>
   <to-view-id>/protected/admin/topicMenue.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>subcategoryMenue</from-outcome>
   <to-view-id>/protected/admin/subcategoryMenue.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/logout.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/videoVerwaltung.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>topicMenue</from-outcome>
   <to-view-id>/protected/admin/topicMenue.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>videoAdministration</from-outcome>
   <to-view-id>/protected/admin/videoAdministration.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>videoSuche</from-outcome>
   <to-view-id>/protected/admin/videoSuche.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>zurueckZurStartseite</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/videoSuche.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>videoSuche</from-outcome>
   <to-view-id>/protected/admin/videoSicht.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>zurueckZurVideoverwaltung</from-outcome>
   <to-view-id>/protected/admin/videoVerwaltung.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/videoAdministration.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>bestehendesVideoAendern</from-outcome>
   <to-view-id>/protected/admin/videoAendern.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>zurueckZurVideoverwaltung</from-outcome>
   <to-view-id>/protected/admin/videoVerwaltung.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>zurueckZumVideoMenue</from-outcome>
   <to-view-id>/protected/admin/videoAdministration.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/videoAendern.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>zurueckZurVideoverwaltung</from-outcome>
   <to-view-id>/protected/admin/videoVerwaltung.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/videoSicht.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>zurueckZurVideoverwaltung</from-outcome>
   <to-view-id>/protected/admin/videoVerwaltung.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>loginError.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToLogin</from-outcome>
   <to-view-id>login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>logout.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToLogin</from-outcome>
   <to-view-id>login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/userMenue.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToStartingPage</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createNewUser</from-outcome>
   <to-view-id>/protected/admin/userCreate.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changeExistingUser</from-outcome>
   <to-view-id>/protected/admin/userChange.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/userChange.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToUserMenue</from-outcome>
   <to-view-id>/protected/admin/userMenue.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/userCreate.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToUserMenue</from-outcome>
   <to-view-id>/protected/admin/userMenue.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/topicMenue.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToStartPage</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createNewTopic</from-outcome>
   <to-view-id>/protected/admin/topicAnlegen.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changeExistingTopic</from-outcome>
   <to-view-id>/protected/admin/topicAendern.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showSubcategories</from-outcome>
   <to-view-id>/protected/admin/subcategoriesByTopic.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/topicAendern.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToTopicMenue</from-outcome>
   <to-view-id>/protected/admin/topicMenue.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/topicAnlegen.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToTopicMenue</from-outcome>
   <to-view-id>/protected/admin/topicMenue.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/subcategoryMenue.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToStartPage</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createNewSubcategory</from-outcome>
   <to-view-id>/protected/admin/subcategoryAnlegen.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changeExistingSubcategory</from-outcome>
   <to-view-id>/protected/admin/subcategoryAendern.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/subcategoryAnlegen.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToSubcategoryMenue</from-outcome>
   <to-view-id>/protected/admin/subcategoryMenue.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/subcategoryAendern.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToSubcategoryMenue</from-outcome>
   <to-view-id>/protected/admin/subcategoryMenue.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/subcategoriesByTopic.xhtml</from-view-id>
 </navigation-rule>
 <application/>
</faces-config>
