<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>userDataMB</managed-bean-name>
  <managed-bean-class>de.mb.UserDataMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>topicMB</managed-bean-name>
  <managed-bean-class>de.mb.TopicMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>subcategoryMB</managed-bean-name>
  <managed-bean-class>de.mb.SubcategoryMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>videoMB</managed-bean-name>
  <managed-bean-class>de.mb.VideoMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>configurationMB</managed-bean-name>
  <managed-bean-class>de.mb.ConfigurationMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/protected/public/startseite.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>userMenu</from-outcome>
   <to-view-id>/protected/admin/userMenu.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>videoMenu</from-outcome>
   <to-view-id>/protected/admin/videoMenu.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>topicMenu</from-outcome>
   <to-view-id>/protected/admin/topicMenu.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>subcategoryMenu</from-outcome>
   <to-view-id>/protected/admin/subcategoryMenu.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/logout.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/videoMenu.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>videoAdministration</from-outcome>
   <to-view-id>/protected/admin/videoAdministration.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>videoSearchAll</from-outcome>
   <to-view-id>/protected/public/videoSearchAll.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>backToStartPage</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/public/videoSearchAll.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>videoWatch</from-outcome>
   <to-view-id>/protected/public/videoWatch.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>backToStartPage</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/videoAdministration.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>changeVideo</from-outcome>
   <to-view-id>/protected/admin/videoChange.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>backToVideoMenu</from-outcome>
   <to-view-id>/protected/admin/videoMenu.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/videoChange.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToVideoAdministration</from-outcome>
   <to-view-id>/protected/admin/videoAdministration.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/public/videoWatch.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToVideoSearchAll</from-outcome>
   <to-view-id>/protected/public/videoSearchAll.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>loginError.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToLogin</from-outcome>
   <to-view-id>login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>logout.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToLogin</from-outcome>
   <to-view-id>login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/userMenu.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToStartingPage</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createNewUser</from-outcome>
   <to-view-id>/protected/admin/userCreate.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changeExistingUser</from-outcome>
   <to-view-id>/protected/admin/userChange.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/userChange.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToUserMenu</from-outcome>
   <to-view-id>/protected/admin/userMenu.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/userCreate.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToUserMenu</from-outcome>
   <to-view-id>/protected/admin/userMenu.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/topicMenu.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToStartPage</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createNewTopic</from-outcome>
   <to-view-id>/protected/admin/topicCreate.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changeExistingTopic</from-outcome>
   <to-view-id>/protected/admin/topicChange.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showSubcategories</from-outcome>
   <to-view-id>/protected/admin/subcategoriesByTopic.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/topicChange.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToTopicMenu</from-outcome>
   <to-view-id>/protected/admin/topicMenu.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/topicCreate.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToTopicMenu</from-outcome>
   <to-view-id>/protected/admin/topicMenu.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/subcategoryMenu.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToStartPage</from-outcome>
   <to-view-id>/protected/public/startseite.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createNewSubcategory</from-outcome>
   <to-view-id>/protected/admin/subcategoryCreate.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changeExistingSubcategory</from-outcome>
   <to-view-id>/protected/admin/subcategoryChange.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/subcategoryCreate.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToSubcategoryMenu</from-outcome>
   <to-view-id>/protected/admin/subcategoriesByTopic.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/subcategoryChange.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToSubcategoryMenu</from-outcome>
   <to-view-id>/protected/admin/subcategoriesByTopic.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/protected/admin/subcategoriesByTopic.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>backToTopicMenu</from-outcome>
   <to-view-id>/protected/admin/topicMenu.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>changeExistingSubcategory</from-outcome>
   <to-view-id>/protected/admin/subcategoryChange.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>createNewSubcategory</from-outcome>
   <to-view-id>/protected/admin/subcategoryCreate.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
